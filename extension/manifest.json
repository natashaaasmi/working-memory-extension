{
    "manifest_version": 3,
    "name": "MemoryWeb",
    "description": "An extension that helps you remember what you learn",
    "version": "1.0",
    "background":{
      "service_worker":"background.js"
    },
    "side_panel": {
      "default_path":"hello.html"
    },
    "permissions":["sidePanel","activeTab","nativeMessaging"],
    "content_scripts": [
      {
        "matches":["<all_urls>"],
        "js":["content.js"] 
      }
    ]
  }
